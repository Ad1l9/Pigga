@model List<Setting>
@{
    int count = 0;
}


<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Employee Table</h4>
            <a class="btn btn-primary" asp-action="Create">Create Setting</a>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>No</th>
                            <th>Key</th>
                            <th>Value</th>
                            <th>Settings</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (Setting setting in Model)
                        {
                            count++;
                            <tr>
                                <td>@count</td>

                                <td>@setting.Key</td>
                                <td>@setting.Value</td>
                                <td>
                                    <a class="btn btn-warning" asp-action="Update" asp-route-id="@setting.Id">Update</a>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>